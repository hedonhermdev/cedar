use clap::Parser;
use cedar_db::db::DbError;
use cedar_db::server::Server;

#[derive(Parser, Debug)]
struct Arguments {
    #[clap(short, long)]
    db: String,

    #[clap(short, long, default_value = "3030")]
    port: u16,
}

#[tokio::main]
async fn main() -> Result<(), DbError> {
    let args = Arguments::parse();

    let db = match args.db.as_str() {
        "DuckDB" => todo!(),
        _ => panic!("Unknown database"),
    };


    Ok(())
}
